        Object day = workingDays.get(0);
        int dayy = Integer.parseInt(String.valueOf(day));
        LocalDate localDate = LocalDate.now().minusMonths(1).withDayOfMonth(dayy);
        String targetDate = localDate.toString();
        vars.put(&quot;DATE_FOR_SHIFT&quot;, targetDate);    
