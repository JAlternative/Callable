        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="Переименование 15 файлов" enabled="true">
          <stringProp name="BeanShellSampler.query">
	   ArrayList filesInFolder = new ArrayList();

        File folder = new File(&quot;10004//&quot;);
        File[] files = folder.listFiles();
        for (File file : files) {
            if (file.isFile()) {
                filesInFolder.add(file);
            }
        }

       ArrayList idList = new ArrayList();
       Integer count = Integer.valueOf(vars.get(&quot;USERS&quot;))+1;
       for (int i=1; i&lt;count; i++){
            p = vars.get(&quot;id_&quot;+i);
            idList.add(p);
       }


        ArrayList idsToRemove = new ArrayList();
        ArrayList filesToRemove = new ArrayList();
        for (File file : filesInFolder) {
            String fileName = file.getName();
            String fileByID = fileName.substring(0, fileName.indexOf(&quot;.&quot;));
            if (idList.contains(fileByID)) {
                filesToRemove.add(file);
                idsToRemove.add(fileByID);
            }
        }

        filesInFolder.removeAll(filesToRemove);
        idList.removeAll(idsToRemove);
        for (int i = 0; i &lt; idList.size(); i++) {
            File oldFile = filesInFolder.get(i);
            File newFile = new File(&quot;10004//&quot; + idList.get(i) + &quot;.jpg&quot;);
            oldFile.renameTo(newFile);
        }</stringProp>
          <stringProp name="BeanShellSampler.filename"></stringProp>
