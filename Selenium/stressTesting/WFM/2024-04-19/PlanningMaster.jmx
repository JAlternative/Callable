import java.io.File;
import java.io.FileWriter;
import java.io.IOException;
import java.time.LocalDate;
import java.time.format.DateTimeFormatter;

String filePath = &quot;saas.csv&quot;;
Integer value = Integer.parseInt(vars.get(&quot;employeeId_matchNr&quot;));

FileWriter fw = null;
try {
    fw = new FileWriter(filePath, true);
    File file = new File(filePath);
    if (file.length() == 0) {
        fw.write(&quot;omId,employeeId\n&quot;);
    }

    for (int a = 1; a &lt;= value; a++) {
    	   String variable = &quot;employeeId_&quot; + a;
        fw.write(vars.get(&quot;orgUnitId&quot;) + &quot;,&quot; + vars.get(variable) + &quot;\n&quot;);
    }
    fw.close();
} catch (IOException e) {
    e.printStackTrace();
}

