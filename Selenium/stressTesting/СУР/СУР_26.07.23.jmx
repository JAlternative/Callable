        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Записать токен в csv файл" enabled="true">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import java.io.BufferedReader;
import java.io.BufferedWriter;
import java.io.FileReader;
import java.io.FileWriter;
import java.io.IOException;

        String filePath = &quot;token.csv&quot;;
        String newLineData = &quot;${TOKEN}&quot;;

        try {
            BufferedReader reader = new BufferedReader(new FileReader(filePath));
            StringBuilder content = new StringBuilder();

            // Читаем и пропускаем первую строку
            reader.readLine();

            String line;
            while ((line = reader.readLine()) != null) {
                content.append(line).append(System.lineSeparator());
            }
            reader.close();

            BufferedWriter writer = new BufferedWriter(new FileWriter(filePath));
            writer.write(newLineData + System.lineSeparator());
            writer.write(content.toString());
            writer.close();
        } catch (IOException e) {
            e.printStackTrace();
        }
</stringProp>
          <stringProp name="scriptLanguage">java</stringProp>
        </JSR223Sampler>
