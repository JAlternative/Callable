datesToMonthEnd.add(date.toString());
while (!endOfMonth.equals(date)) {
	datesToMonthEnd.add(endOfMonth.toString());
	endOfMonth = endOfMonth.minusDays(1);
}
int counter = 1;
for (int i = 0 ; i &lt; datesToMonthEnd.size(); i++) {
