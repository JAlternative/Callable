<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Модуль Применение ролей. Массовый расчет, Категории позиций, Типы позиций, Мат.параметры" thread-count="4" parallel="classes">
    <listeners>
        <listener class-name="testng.retry.AnnotationTransformer"/>
    </listeners>
    <test name="Массовый расчет">
        <groups>
            <run>
                <include name="TK2050-2.1"/>
                <include name="TK2050-2.3"/>
                <include name="TK2050-2.4"/>
                <include name="TK2050-2.5"/>
                <include name="TK2050-2.6"/>
                <include name="TK2050-3.1"/>
                <include name="TK2050-3.3"/>
                <include name="TK2050-3.4"/>
                <include name="TK2050-3.5"/>
                <include name="TK2050-3.6"/>
                <include name="TK2050-4.1"/>
                <include name="TK2050-4.3"/>
                <include name="TK2050-4.4"/>
                <include name="TK2050-4.5"/>
                <include name="TK2050-4.6"/>
            </run>
        </groups>
        <classes>
            <class name="pagemodel.BatchCalculation"/>
        </classes>
    </test>
    <test name="Математические параметры">
        <groups>
            <run>
                <include name="TK2051-2-1"/>
                <include name="TK2051-3.1"/>
            </run>
        </groups>
        <classes>
            <class name="pagemodel.MathParameters"/>
        </classes>
    </test>
    <test name="Типы позиций">
        <groups>
            <run>
                <include name="TK2052-1.2.1"/>
                <include name="TK2051-1.2.2"/>
                <include name="TK2051-2.1.1"/>
                <include name="TK2051-2.1.2"/>
                <include name="TK2051-2.2.1"/>
                <include name="TK2051-3.1"/>
                <include name="TK2051-3.2"/>
                <include name="TK2051-4.1"/>
                <include name="TK2051-4.2"/>
                <include name="TK2051-5.1"/>
                <include name="TK2051-5.2"/>
            </run>
        </groups>
        <classes>
            <class name="pagemodel.MathParameters"/>
        </classes>
    </test>
</suite>