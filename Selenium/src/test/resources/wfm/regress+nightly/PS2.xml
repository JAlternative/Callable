<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Личное расписание" thread-count="2" parallel="tests">
    <listeners>
        <listener class-name="testng.retry.AnnotationTransformer"/>
    </listeners>
    <test name="PS2">
        <groups>
            <run>
                <include name="PS2"/>
                <exclude name="in progress"/>
                <exclude name="grade super0"/>
                <exclude name="grade 3"/>
                <exclude name="pochta"/>
                <exclude name="zozo"/>
                <exclude name="magnit"/>
                <exclude name="efes"/>
                <exclude name="x5"/>
            </run>
        </groups>
        <classes>
            <class name="apitest.PersonalScheduleApiTest"/>
            <class name="pagemodel.PersonalSchedule"/>
        </classes>
    </test>
</suite>