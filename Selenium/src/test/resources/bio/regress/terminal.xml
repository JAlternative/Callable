<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Изменения параметров функционирования терминала" thread-count="2" parallel="tests">
    <test name="Настройки терминала">
        <groups>
            <run>
                <include name="BI-4.[1, 4, 5, 6, 7, 9]"/>
                <include name="BI-4.10"/>
                <include name="BI-4.14"/>
            </run>
        </groups>
        <classes>
            <class name="pagemodel.BioControl"/>
        </classes>
    </test>
    <test name="Редактирование привязанного персонала к терминалу">
        <groups>
            <run>
                <include name="BI-4.15"/>
                <include name="BI-4.16"/>
                <include name="BI-4.11.1"/>
                <include name="BI-4.11.2"/>
                <include name="BI-4.12.1"/>
                <include name="BI-4.12.2"/>
            </run>
        </groups>
        <classes>
            <class name="pagemodel.BioControl"/>
        </classes>
    </test>
</suite>